<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="dt" class="java.util.Date"/>
    <bean id="dt2" class="java.util.Date">
    	<property name="year" value="110"/>
    </bean>
    
<!--     test by giving case sensitive property name.
    test by giving case sensitive constructor name. -->
    
   <!--  check whether two object creates or two threads create by two bean id of same class:
     1) in case of setter injection
     2) in case of constructor injection -->
     
     <!-- testing of private constructor and private setter method -->
    
    
    <bean id="wmg" class="com.nt.beans.WishMessageGenerator">
    	<property name="date" ref="dt2"></property>
    	<constructor-arg name="date" ref="dt"></constructor-arg>
    </bean>

</beans>